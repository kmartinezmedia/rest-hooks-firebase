{
  "name": "ui-kit",
  "version": "0.0.1",
  "description": "ui-kit - An Anansi project",
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx",
    "format": "yarn run lint --fix",
    "type-check": "tsc",
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "npm test -- --ci",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "webpack-dev-server --mode=development",
    "prod": "serve ./generated_assets",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --analyze",
    "profile": "webpack --mode=production --profile",
    "pkgcheck": "webpack --check=nobuild"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmartinezmedia/ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/kmartinezmedia/ui-kit/issues"
  },
  "homepage": "https://github.com/kmartinezmedia/ui-kit#readme",
  "keywords": [
    "anansi"
  ],
  "main": "src/index.ts",
  "types": "src/index.ts",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@anansi/babel-preset": "^1.1.0",
    "@anansi/eslint-plugin": "^0.8.4",
    "@anansi/webpack-config": "^0.19.6",
    "@babel/core": "^7.8.4",
    "@hot-loader/react-dom": "^16.0.0",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-info": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "babel-jest": "^25.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "react-refresh": "^0.7.0",
    "serve": "^11.3.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5",
    "webpack": "^4.36.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "firebase": "^7.8.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rest-hooks": "^4.5.0"
  }
}
